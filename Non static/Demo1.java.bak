class Demo1 
{
    static void triangle(int b, int h) 
	{
        double res1=0.5*b*h;
        System.out.println(res1);
    }

    static void rectangle(int w, int h) 
    {
        int res2=w*h;
        System.out.println(res2);
     }

    static void trapezoid(int a, int b, int h) 
	{
        double res3=0.5*(a+b)*h;
        System.out.println(res3);
    }

    static void ellipse(int a, int b) 
	{
        final double pi=3.142;
        double res4=pi*a*b;
        System.out.println(res4);
    }

    static void square(int a)
	{
        int res5=a*a;
        System.out.println(res5);
    }

    static void parallelogram(int b, int h) 
	{
        int res6=b*h;
        System.out.println(res6);
    }

    static void circle(int r) 
	{
        final double pi = 3.142;
        double res7=pi*r*r;
        System.out.println(res7);
    }

    static void sector(int r) 
	{
        double t=30.5;
        double res8=0.5*r*r*t;
        System.out.println(res8);
    }

    public static void main(String[] args) 
		{
        System.out.println("Triangle");
        Assignment.triangle(5,7);

        System.out.println("Rectangle");
        Assignment.rectangle(10,5);

        System.out.println("Trapezoid");
        Assignment.trapezoid(10,18,5);

        System.out.println("Ellipse");
        Assignment.ellipse(5,5);

        System.out.println("Square");
        Assignment.square(5);

        System.out.println("Parallelogram");
        Assignment.parallelogram(10,5);

        System.out.println("Circle");
        Assignment.circle(5);

        System.out.println("Sector");
        Assignment.sector(5);
    }
}